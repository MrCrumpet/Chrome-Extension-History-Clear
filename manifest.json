{
	"name":"Experiment",
	"description":"testing project ideas",
	"version":"1.0",

	"manifest_version": 2,


	"browser_action": {
		"default_popup":"popup.html"
	},

	"icons": { "16": "icon16.png",
			   "48": "icon48.png",
			   "128": "icon128.png" },

	"content_scripts":
	[
	  {
	    "matches": ["*://*/*"],
	    "js": ["popup.js"],
	    "run_at": "document_end"
	  }
	],

	"permissions": [
		"tabs",
		"storage"
	]
}